
		case html.SelfClosingTagToken:
			if!skipClosingTag {
				buff.WriteString(token.Data)
			} else { // skip closing tag
				break
			}

		case html.TextToken:
			if!skipElementContent {
				buff.WriteString(token.Data)
			} else { // skip element content
				break
			}

		case html.CommentToken:
			if!skipElementContent {
				buff.WriteString(token.Data)
			} else { // skip element content
				break
			}

		case html.EntityReferenceToken:
			if!skipElementContent {
				buff.WriteString(token.Data)
			} else { // skip element content
				break
			}

		case html.EntityReferenceToken:
			if!skipElementContent {
				buff.WriteString(token.Data)
			} else { // skip element content
				break
			}
		}

		case html.EntityReferenceToken:
			if!skipElementContent {
				buff.WriteString(token.Data)
			break
			break
		}
		break
	}
	}

	case html.EntityReferenceToken:
			if!skipElementContent {
			buff.WriteString(token.Data)
			break
		break
	}
	}

	case html.EntityReferenceToken:
			break
		break
	}
	break
	break

	case html.EntityReferenceToken:
			break
	break
	break
	break
	break
	break
	break

	break
	break
	break
	break
	break
}
	break
	break
	break
	break
	break
	break
	break
	break
	break
	break

